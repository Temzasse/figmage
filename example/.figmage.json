{
  "outDir": "./example/tokens",
  "tokenize": {
    "tokens": [
      {
        "name": "colors",
        "type": "color"
      },
      {
        "name": "typography",
        "type": "text"
      },
      {
        "name": "shadows",
        "type": "drop-shadow"
      },
      {
        "name": "spacing",
        "type": "width",
        "nodeId": "110:694"
      },
      {
        "name": "sizing",
        "type": "height",
        "nodeId": "102:7"
      },
      {
        "name": "radii",
        "type": "radius",
        "nodeId": "102:8"
      },
      {
        "name": "icons",
        "type": "svg",
        "nodeId": "102:2"
      },
      {
        "name": "assets",
        "type": "png",
        "nodeId": "643:618"
      }
    ]
  },
  "codegen": {
    "defaults": {
      "filetype": "ts",
      "tokenCase": "camel"
    },
    "assets": {
      "filetype": "png",
      "tokenCase": "kebab"
    },
    "colors": {
      "exclude": {
        "group": ["Env"],
        "token": "\\b(Figma)\\b.*"
      }
    },
    "sizing": {
      "exclude": {
        "token": "\\b(Icon Size)\\b.*"
      }
    }
  }
}
