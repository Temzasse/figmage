{
  "outDir": "./example/tokens",
  "tokenize": {
    "tokens": [
      {
        "name": "colors",
        "type": "color"
      },
      {
        "name": "typography",
        "type": "text"
      },
      {
        "name": "shadows",
        "type": "drop-shadow"
      },
      {
        "name": "spacing",
        "type": "width",
        "nodeName": "Figmage - Spacing"
      },
      {
        "name": "radii",
        "type": "radius",
        "nodeName": "Figmage - Radii"
      },
      {
        "name": "icons",
        "type": "svg",
        "nodeName": "Figmage - Icons"
      }
    ]
  },
  "codegen": {
    "defaults": {
      "filetype": "ts",
      "tokenCase": "camel"
    },
    "colors": {
      "exclude": {
        "group": ["Env"],
        "token": "\\b(Figma)\\b.*"
      }
    }
  }
}
